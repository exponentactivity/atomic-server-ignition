{
  "ignition": {
    "version": "3.5.0"
  },
  "passwd": {
    "users": [
      {
        "name": "containers",
        "shell": "/sbin/nologin",
        "uid": 2000
      }
    ]
  },
  "storage": {
    "directories": [
      {
        "group": {
          "name": "containers"
        },
        "path": "/home/containers/.config",
        "user": {
          "name": "containers"
        },
        "mode": 493
      },
      {
        "group": {
          "name": "containers"
        },
        "path": "/home/containers/.config/systemd",
        "user": {
          "name": "containers"
        },
        "mode": 493
      },
      {
        "group": {
          "name": "containers"
        },
        "path": "/home/containers/.config/systemd/user",
        "user": {
          "name": "containers"
        },
        "mode": 493
      },
      {
        "group": {
          "name": "containers"
        },
        "path": "/home/containers/.config/systemd/user/default.target.wants",
        "user": {
          "name": "containers"
        },
        "mode": 493
      }
    ],
    "files": [
      {
        "path": "/var/lib/systemd/linger/containers",
        "mode": 420
      },
      {
        "group": {
          "name": "containers"
        },
        "path": "/home/containers/.config/systemd/user/chezmoi-initialize.service",
        "user": {
          "name": "containers"
        },
        "contents": {
          "compression": "gzip",
          "source": "data:;base64,"
        },
        "mode": 420
      }
    ],
    "links": [
      {
        "group": {
          "name": "containers"
        },
        "path": "/home/containers/.config/systemd/user/default.target.wants/chezmoi-initialize.service",
        "user": {
          "name": "containers"
        },
        "hard": false,
        "target": "/home/containers/.config/systemd/user/chezmoi-initialize.service"
      }
    ]
  }
}
